                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.332" />

            <Button
                android:id="@+id/botonPlayaDC"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="322dp"
                android:layout_height="59dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="informacion"
                android:textAlignment="center"
                android:textColor="@color/rob_roy"
                app:iconGravity="top|end"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.397" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/cancun"
        android:layout_width="409dp"
        android:layout_height="97dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.995">

        <ImageView
            android:id="@+id/imagen5"
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/cancun2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nombreCancun"
                style="@style/Widget.AppCompat.TextView"
                android:layout_width="322dp"
                android:layout_height="36dp"
                android:gravity="center"
                android:text="HUELLITAS | CANCUN"
                android:textAllCaps="true"
                android:textColor="@color/burnt_sienna"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.332" />

            <Button
                android:id="@+id/botonCancun"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="322dp"
                android:layout_height="59dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="informacion"
                android:textAlignment="center"
                android:textColor="@color/rob_roy"
                app:iconGravity="top|end"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.397" />
