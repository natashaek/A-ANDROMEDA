<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blue_dianne"
    tools:context=".Centros">

    <LinearLayout
        android:id="@+id/fcp"
        android:layout_width="409dp"
        android:layout_height="97dp"
        android:layout_marginTop="15dp"
        android:animateLayoutChanges="false"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/imagen"
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/fcp2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nombreFcp"
                style="@style/Widget.AppCompat.TextView"
                android:layout_width="322dp"
                android:layout_height="36dp"
                android:gravity="center"
                android:text="ADOPTAME | FCP"
                android:textAllCaps="true"
                android:textColor="@color/burnt_sienna"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/botonFcp"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="322dp"
                android:layout_height="59dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="informacion"
                android:textAlignment="center"
                android:textColor="@color/rob_roy"
                app:iconGravity="top|end" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/chetumal"
        android:layout_width="409dp"
        android:layout_height="97dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.197">

        <ImageView
            android:id="@+id/imagen2"
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/chetumal2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nombreChetu"
                style="@style/Widget.AppCompat.TextView"
                android:layout_width="322dp"
                android:layout_height="36dp"
                android:gravity="center"
                android:text="MI MEJOR AMIGO | CHETUMAL"
                android:textAllCaps="true"
                android:textColor="@color/burnt_sienna"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/botonChetu"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="322dp"
                android:layout_height="59dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="informacion"
                android:textAlignment="center"
                android:textColor="@color/rob_roy"
                app:iconGravity="top|end" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bacalar"
        android:layout_width="409dp"
        android:layout_height="97dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.388">

        <ImageView
            android:id="@+id/imagen7"
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/bacalar" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nombreBacalar"
                style="@style/Widget.AppCompat.TextView"
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:gravity="center"
                android:text="PET HOUSE | BACALAR"
                android:textAllCaps="true"
                android:textColor="@color/burnt_sienna"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/botonBacalar"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="match_parent"
                android:layout_height="59dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="informacion"
                android:textAlignment="center"
                android:textColor="@color/rob_roy"
                app:iconGravity="top|end" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/tulum"
        android:layout_width="409dp"
        android:layout_height="97dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fcp"
        app:layout_constraintVertical_bias="0.585">

        <ImageView
            android:id="@+id/imagen3"
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/tulum2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nombreTulum"
                style="@style/Widget.AppCompat.TextView"
                android:layout_width="322dp"
                android:layout_height="36dp"
                android:gravity="center"
                android:text="TRUE LOVE | TULUM"
                android:textAllCaps="true"
                android:textColor="@color/burnt_sienna"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/botonTulum"
                style="@style/Widget.AppCompat.Button.Borderless"
                android:layout_width="322dp"
                android:layout_height="59dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="informacion"
                android:textAlignment="center"
                android:textColor="@color/rob_roy"
                app:iconGravity="top|end" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/playaDC"
        android:layout_width="409dp"
        android:layout_height="97dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.791">

        <ImageView
            android:id="@+id/imagen4"
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.36"
            app:srcCompat="@drawable/playadelcarmen2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:baselineAligned="false"
            android:orientation="vertical">

            <TextView
                android:id="@+id/nombrePlayaDC"
                style="@style/Widget.AppCompat.TextView"
                android:layout_width="322dp"
                android:layout_height="36dp"
                android:gravity="center"
                android:text="+KOTAS | PLAYA DEL CARMEN"
                android:textAllCaps="true"
                android:textColor="@color/burnt_sienna"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
